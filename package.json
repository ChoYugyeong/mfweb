  "keywords": ["trpg", "cinema", "vintage", "supabase"],
  "author": "Yugyeong",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "rollup": "^3.29.4",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "autoprefixer": "^10.4.16",
    "live-server": "^1.2.2"
  }
}