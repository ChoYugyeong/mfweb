{
  "name": "yugyeong-cinema",
  "version": "1.0.0",
  "description": "Yugyeong's Cinema - A vintage cinema-themed TRPG board",
  "main": "src/js/app.js",
  "scripts": {
    "dev": "live-server public --port=3000",
    "build": "npm run build:js && npm run build:css",
    "build:js": "rollup -c",
    "build:css": "postcss public/styles/main.css -o public/styles/bundle.css",
    "compile:ts": "tsc"
